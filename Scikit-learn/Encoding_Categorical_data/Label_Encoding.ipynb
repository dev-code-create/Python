    "import pandas as pd \n",
    "\n",
    "df = pd.read_csv(\"train.csv\")\n",
    "\n",
    "df_label = df.copy()\n",
    "\n",
    "le = LabelEncoder()\n",
    "\n",
    "df_label['Gender_Encoder'] = le.fit_transform(df_label['Gender'])\n",
    "\n",
    "df_label['Passed_Encoded'] = le.fit_transform(df_label['Passed'])\n",
    "\n"
   ]
